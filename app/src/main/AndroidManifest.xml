        <activity android:name=".Activity.CourseDetailActivity" />
        <activity android:name=".Activity.InterestActivity" />
        <activity android:name=".Activity.FromWhereActivity" />
        <activity android:name=".Activity.DashBoard" />
        <activity android:name=".Activity.IntroActivity" />
        <activity android:name=".Activity.SpalshActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>